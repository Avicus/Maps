      <regions>
        <block>746, 126, -152</block>
      </regions>
    </spawn>
    <spawn team="red" yaw="0">
      <regions>
        <block>851, 70.5, -206</block>
      </regions>
    </spawn>
    <spawn team="blue" yaw="180">
      <regions>
        <block>849, 70.5, -94</block>
      </regions>
    </spawn>
  </spawns>
  <zones>
    <zones>
      <zone modify="never" region="everywhere"/>
