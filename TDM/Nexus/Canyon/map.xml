    <spawn yaw="-90">
      <regions>
        <point yaw="-90">-1278.5, 38, -1148.5</point>
      </regions>
    </spawn>
    <spawn team="orange" yaw="180">
      <regions>
        <point yaw="180">-1282.5, 33, -1100.5</point>
      </regions>
    </spawn>
    <spawn team="red" yaw="0">
      <regions>
        <point yaw="0">-1274.5, 33, -1196.5</point>
      </regions>
    </spawn>
  </spawns>
  <zones>
    <zones>
      <zone modify="never" region="everywhere"/>
